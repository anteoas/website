{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "clojure -Teden watch :site-edn '\"site.edn\"'",
    "watch": "clojure -Teden watch :site-edn '\"site.edn\"'",
    "build": "clojure -Teden build :site-edn '\"site.edn\"'",
    "build:prod": "clojure -Teden build :site-edn '\"site.edn\"' :mode '\"prod\"'",
    "serve": "clojure -Teden serve :block? true",
    "clean": "clojure -Teden clean",
    "mcp-stdio": "clojure -Teden mcp-stdio :site-edn '\"site.edn\"'"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  }
}
